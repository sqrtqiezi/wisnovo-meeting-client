page {
  background-color: #F4F5F9;
}

.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  padding: 20rpx 0;
  justify-content: space-between;
  box-sizing: border-box;
}

.btn-link {
  border-style: none;
  background: #F4F5F9;
  color: #838B95;
  font-size: .8em;
}

.btn-link::after {
  border-style: none;
}